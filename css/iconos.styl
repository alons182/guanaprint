@font-face {
	font-family: 'guanaprint';
	src:url('../fonts/guanaprint.eot');
	src:url('../fonts/guanaprint.eot?#iefix') format('embedded-opentype'),
		url('../fonts/guanaprint.ttf') format('truetype'),
		url('../fonts/guanaprint.woff') format('woff'),
		url('../fonts/guanaprint.svg#guanaprint') format('svg');
	font-weight: normal;
	font-style: normal;
}

[class*="icon-"] {
	font-family: 'guanaprint';
	speak: none;
	font-style: normal;
	font-weight: normal;
	font-variant: normal;
	text-transform: none;
	line-height: 1;

	/* Better Font Rendering =========== */
	-webkit-font-smoothing: antialiased;
	-moz-osx-font-smoothing: grayscale;
}

.icon-home:before {
	content: "\e600";
}
.icon-stack:before {
	content: "\e601";
}
.icon-users:before {
	content: "\e602";
}
.icon-phone:before {
	content: "\e603";
}
.icon-upload:before {
	content: "\e604";
}
.icon-menu:before {
	content: "\e605";
}
.icon-list:before {
	content: "\e606";
}


